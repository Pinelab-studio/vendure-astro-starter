{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "plugins": [
      {
        "name": "gql.tada/ts-plugin",
        "schema": "https://vendure.noharmdone.nl/shop-api",
        "tadaOutputLocation": "./src/graphql/graphql-env.d.ts"

      }
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}